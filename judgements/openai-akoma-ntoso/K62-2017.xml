            xmlns="http://docs.oasis-open.org/legaldocml/ns/akn/3.0/WD17">
    <judgment>
        <meta>
            <identification source="#court">
                <FRBRWork>
                    <FRBRauthor>OSNOVNI SUD U PODGORICI</FRBRauthor>
                    <FRBRdate date="2017-05-12">2017-05-12</FRBRdate>
                    <FRBRtitle>K.br. 62/17</FRBRtitle>
